    </tr>
    {{#each this.properties}}
        <tr>
            <td>{{@key}}
                {{#if vendorExtensions.x-deprecated}}<span style="color:red">DEPRECATED</span> {{/if}}</td>
            <td>
                {{#ifeq type "array"}}
                    {{#items.$ref}}
                        {{type}}[<a href="{{items.$ref}}">{{basename items.$ref}}</a>]
                    {{/items.$ref}}
                    {{^items.$ref}}{{type}}[{{items.type}}]{{/items.$ref}}

                {{else}}
                    {{!-- not primitive type --}}
                    {{#$ref}}<a href="{{$ref}}">{{basename $ref}}</a>{{/$ref}}
                    {{!-- primitive type --}}
                    {{^$ref}}{{type}}
                        {{#format}} ({{format}}){{/format}}
                        {{#if enum}}- Value must be in: {{join enum ", "}}{{/if}}
                    {{/$ref}}
                {{/ifeq}}
            </td>
            <td>{{#required}}required{{/required}}{{^required}}optional{{/required}}</td>
            <td>{{#description}}{{{description}}}{{/description}}{{^description}}-{{/description}}</td>
            <td>{{example}}</td>
        </tr>
    {{/each}}
</table>
